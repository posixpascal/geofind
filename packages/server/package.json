{
  "name": "@geofind/server",
  "version": "1.0.0",
  "description": "A node websocket server for the geographical online guessing game geofind.io",
  "main": "./build/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/posixpascal/geofind_server"
  },
  "author": "Pascal Raszyk",
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node build/server.js",
    "watch-node": "nodemon build/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "./node_modules/.bin/tsc",
    "watch-ts": "./node_modules/.bin/tsc -w",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.{ts,tsx}'",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@colyseus/monitor": "^0.11.0",
    "@colyseus/schema": "1.0.16",
    "@colyseus/social": "^0.11.6",
    "@google/maps": "^1.1.3",
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "async": "^3.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "chance": "^1.1.0",
    "colyseus": "0.14",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "express-validator": "^6.0.1",
    "fbgraph": "^1.4.4",
    "lodash": "^4.17.13",
    "lusca": "^1.6.1",
    "md5": "^2.2.1",
    "mongoose": "^5.6.0",
    "node-geocoder": "^3.24.0",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "react": "^16",
    "react-dom": "^16.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "socket.io": "^2.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "uifx": "^2.0.3",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/connect-mongo": "^0.0.42",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-flash": "0.0.0",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.134",
    "@types/lusca": "^1.6.0",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
